<Application x:Class="SccefDecryptor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:sccefDecryptor="clr-namespace:SccefDecryptor">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <sccefDecryptor:DecryptorBootstraper x:Key="Bootstrapper"/>
                </ResourceDictionary>
                <ResourceDictionary Source="/SccefDecryptor;component/ResourceDictionaries/ResourceLibrary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>